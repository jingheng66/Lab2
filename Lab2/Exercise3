def display_main_menu():
    print("Enter some numbers seperated by commas (e.g. 5, 67, 32)")

def get_user_input():
    user_input = input()
    str_list = user_input.split(",")

    float_list = [float(num) for num in str_list]
    return float_list

display_main_menu()

# Get the user input and process it
numbers = get_user_input()

# Print the result
print("List of floats:", numbers)

    